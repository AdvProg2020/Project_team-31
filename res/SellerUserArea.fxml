<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane prefHeight="247.0" stylesheets="@/Style.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GraphicalView.SellerUserArea">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="225.0" minWidth="10.0" prefWidth="90.33333333333333" />
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="168.66666666666669" minWidth="10.0" prefWidth="125.00000000000001" />
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="157.0" minWidth="10.0" prefWidth="123.33333333333337" />
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="121.0" minWidth="10.0" prefWidth="111.33333333333331" />
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="114.0" minWidth="10.0" prefWidth="67.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <children>
        <VBox>
            <children>
                <Label fx:id="personalInfo" />
            </children>
        </VBox>
        <GridPane GridPane.columnIndex="1">
            <children>
                <Button fx:id="editPersonaInfo" mnemonicParsing="false" text="edit personal info" GridPane.rowIndex="0" />
                <Button mnemonicParsing="false" onMouseClicked="#salesHistory" text="view sales history" GridPane.rowIndex="1" />
                <Button mnemonicParsing="false" text="view offs" GridPane.rowIndex="2" />
            </children>
            <columnConstraints>
                <ColumnConstraints />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints />
                <RowConstraints />
                <RowConstraints />
                <RowConstraints />
            </rowConstraints>
        </GridPane>
        <GridPane GridPane.columnIndex="2">
            <children>
                <Button mnemonicParsing="false" onMouseClicked="#addProduct" text="add product" GridPane.rowIndex="0" />
                <Button mnemonicParsing="false" text="manage products" GridPane.rowIndex="4" />
                <Button mnemonicParsing="false" onMouseClicked="#showCategories" text="show categories" GridPane.rowIndex="3" />
            </children>
            <columnConstraints>
                <ColumnConstraints />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints />
                <RowConstraints />
                <RowConstraints />
                <RowConstraints />
                <RowConstraints />
            </rowConstraints>
        </GridPane>
        <GridPane GridPane.columnIndex="4">
            <children>
                <Button fx:id="login" mnemonicParsing="false" text="login" GridPane.rowIndex="1" />
                <Button fx:id="logout" mnemonicParsing="false" text="logout" GridPane.rowIndex="2" />
                <Button mnemonicParsing="false" onMouseClicked="#back" text="back" GridPane.rowIndex="3" />
            </children>
            <columnConstraints>
                <ColumnConstraints />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints />
                <RowConstraints />
                <RowConstraints />
                <RowConstraints />
            </rowConstraints>
        </GridPane>
      <GridPane GridPane.columnIndex="3">
         <children>
            <Button mnemonicParsing="false" onAction="#addOff" text="add off" GridPane.rowIndex="0" />
            <Button fx:id="addMeButton" mnemonicParsing="false" onAction="#addMeButtonAlert" text="add me to product" GridPane.rowIndex="1" />
         </children>
         <columnConstraints>
            <ColumnConstraints />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints />
            <RowConstraints />
         </rowConstraints></GridPane>
    </children>
</GridPane>
