<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane stylesheets="@/Style.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GraphicalView.MainMenu">
    <children>
      <VBox fx:id="red" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
              <Label fx:id="welcome" text="welcome to My Shop" />
            <HBox VBox.vgrow="ALWAYS">
               <children>
                    <Button mnemonicParsing="false" onMouseClicked="#products" text="products" />
                    <Button mnemonicParsing="false" onMouseClicked="#offs" text="Offs" />
                    <Button fx:id="userArea" mnemonicParsing="false" onMouseClicked="#userAreaChangeScene" text="User area" />
               </children>
            </HBox>
            <ButtonBar VBox.vgrow="ALWAYS">
               <buttons>
                    <Button fx:id="login" mnemonicParsing="false" text="login" />
                    <Button fx:id="logout" mnemonicParsing="false" text="logout" />
               </buttons>
            </ButtonBar>
         </children>
      </VBox>
    </children>
</AnchorPane>
