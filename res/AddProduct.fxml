<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Pane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GraphicalView.AddProduct">
    <children>
        <GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="0" GridPane.rowIndex="0">
                    <children>
                        <Label text="product name" />
                        <TextField fx:id="firstName" />
                    </children>
                </VBox>
                <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="0" GridPane.rowIndex="1">
                    <children>
                        <Label text="company name" />
                        <TextField fx:id="lastName" />
                    </children>
                </VBox>
                <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="0" GridPane.rowIndex="2">
                    <children>
                        <Label text="price" />
                        <PasswordField fx:id="password" />
                    </children>
                </VBox>
                <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1" GridPane.rowIndex="0">
                    <children>
                        <Label text="description : " />
                        <TextArea prefHeight="96.0" prefWidth="300.0" />
                    </children>
                </VBox>
                <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
                    <children>
                        <Label text="product available number" />
                        <TextField />
                    </children>
                </VBox>
            <HBox GridPane.columnIndex="1" GridPane.rowIndex="2">
               <children>
                      <VBox fx:id="choiceBoxContainer" prefHeight="200.0" prefWidth="200.0">
                     <children>
                        <Label text=" " />
                     </children></VBox>
                  <VBox>
                     <children>
                         <Label text=" " />
                            <Button mnemonicParsing="false" onAction="#back" prefHeight="25.0" prefWidth="97.0" text="back" />
                            <Button mnemonicParsing="false" onAction="#submit" prefHeight="25.0" prefWidth="98.0" text="next" />
                     </children>
                  </VBox>
               </children>
            </HBox>
            <Button mnemonicParsing="false" text="select file"  onMouseClicked="#selectFile" GridPane.columnIndex="0" GridPane.rowIndex="3"/>
            </children>
        </GridPane>
    </children>
</Pane>
